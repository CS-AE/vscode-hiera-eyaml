{
    "name": "vscode-hiera-eyaml",
    "displayName": "vscode-hiera-eyaml",
    "description": "Adds support for hiera-eyaml commands.",
    "version": "0.0.1",
    "publisher": "BrandonTosch",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:hiera-eyaml.Encrypt",
        "onCommand:hiera-eyaml.Decrypt"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "hiera-eyaml.Encrypt",
                "title": "Encrypt YAML"
            },
            {
                "command": "hiera-eyaml.Decrypt",
                "title": "Decrypt YAML"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
